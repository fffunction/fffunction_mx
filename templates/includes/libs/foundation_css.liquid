{% for item in global.fndCss %}
	{% if item.foundation == "true" %}
		{{ "foundation.min.css" | asset_url | stylesheet_tag }}
	{% endif %}
	{% if item.normalize == "true" %}
		{{ "normalize.min.css" | asset_url | stylesheet_tag }}
	{% endif %}
	{% if item.hintCss == "true" %}
		{{ "hint.min.css" | asset_url | stylesheet_tag }}
	{% endif %}
{% endfor %}



